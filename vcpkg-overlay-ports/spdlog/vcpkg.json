{
  "name": "spdlog",
  "version-semver": "1.11.0",
  "port-version": 2,
  "description": "Very fast, header only, C++ logging library",
  "homepage": "https://github.com/gabime/spdlog",
  "license": "MIT",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "fmt"
  ],
  "features": {
    "benchmark": {
      "description": "Use google benchmark",
      "dependencies": [
        "benchmark"
      ]
    },
	"fmt": {
	  "description": "Use fmt as formatting library",
	  "dependencies": [
	    "fmt"
	  ]
	},
    "stdformat": {
      "description": "Use std::format instead of fmt library."
    },
    "wchar": {
      "description": "Build with wchar_t (Windows only)"
    }
  }
}
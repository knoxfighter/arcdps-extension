{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "arcdps-extension",
  "version-string": "1.3.0",
  "dependencies": [
    "magic-enum",
    "nlohmann-json",
    "gtest"
  ],
  "default-features": [
    "imgui",
    "unofficial-extras",
    "curl"
  ],
  "features": {
      "imgui": {
        "description": "make imgui tools available",
        "dependencies": [
          "imgui"
        ]
      },
      "unofficial-extras": {
        "description": "make tools available, that depend on arcdps-unofficial-extras",
        "dependencies": [
          "arcdps-unofficial-extras"
        ]
      },
      "curl":{
        "description": "make tools available, that depend on curl",
        "dependencies": [
          {
            "name": "curl",
            "default-features": false,
            "features": [
              "non-http",
              "openssl"
            ]
          }
        ]
      }
    },
  "overrides": [
    {
      "name": "imgui",
      "version": "1.80#1"
    }
  ]
}
